{
  "name": "react-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^2.0.1",
    "@umijs/max": "^4.0.42",
    "antd": "^5.0.0",
    "qiankun": "^2.7.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint-config-custom": "workspace:*",
    "ui": "workspace:*",
    "lint-staged": "^13.0.3",
    "prettier": "latest",
    "prettier-plugin-organize-imports": "^2",
    "prettier-plugin-packagejson": "^2",
    "tsconfig": "workspace:*",
    "turbo": "latest",
    "typescript": "^4.1.2"
  },
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "node": ">=14.0.0"
  }
}
